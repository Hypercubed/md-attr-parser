{
  "name": "md-attr-parser",
  "version": "1.1.5",
  "description": "A parser for markdown's attributes",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "xo",
    "prepare": "del-cli dist && cross-env BABEL_ENV=production babel src --out-dir dist",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "https://git.ache.one/md-attr-parser://git.ache.one/md-attr-parser/"
  },
  "keywords": [
    "markdown",
    "attribute"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "xo": "^0.18.2"
  },
  "license": "GPL-3.0",
  "xo": {
    "space": true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-var": "warn",
      "object-shorthand": "warn"
    }
  },
  "author": "ache <ache@ache.one>"
}
